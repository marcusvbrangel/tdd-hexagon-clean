{
  "uid": "retail-store-service-detail",
  "title": "Retail Store - Service Detail",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "retail-store"
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "label": "Service",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "refresh": 1,
        "includeAll": false,
        "multi": false,
        "query": "label_values(http_server_requests_seconds_count, service)"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests per second",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"$service\"}[5m]))",
          "refId": "A",
          "instant": false
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Requests by status",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (status) (rate(http_server_requests_seconds_count{service=\"$service\"}[5m]))",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{status}}"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "normal"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Latency p50/p95/p99 (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_server_requests_seconds_bucket{service=\"$service\"}[5m])))",
          "refId": "A",
          "instant": false,
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{service=\"$service\"}[5m])))",
          "refId": "B",
          "instant": false,
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{service=\"$service\"}[5m])))",
          "refId": "C",
          "instant": false,
          "legendFormat": "p99"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Top 5 slow endpoints (p95)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "topk(5, histogram_quantile(0.95, sum by (le, uri) (rate(http_server_requests_seconds_bucket{service=\"$service\"}[5m]))))",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{uri}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "CPU usage (%)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "process_cpu_usage{service=\"$service\"} * 100",
          "refId": "A",
          "instant": false
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 6,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Heap used vs max",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{service=\"$service\",area=\"heap\"}",
          "refId": "A",
          "instant": false,
          "legendFormat": "used"
        },
        {
          "expr": "jvm_memory_max_bytes{service=\"$service\",area=\"heap\"}",
          "refId": "B",
          "instant": false,
          "legendFormat": "max"
        }
      ],
      "gridPos": {
        "x": 18,
        "y": 8,
        "w": 6,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Hikari connections",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "hikaricp_connections_active{service=\"$service\"}",
          "refId": "A",
          "instant": false,
          "legendFormat": "active"
        },
        {
          "expr": "hikaricp_connections_idle{service=\"$service\"}",
          "refId": "B",
          "instant": false,
          "legendFormat": "idle"
        },
        {
          "expr": "hikaricp_connections_max{service=\"$service\"}",
          "refId": "C",
          "instant": false,
          "legendFormat": "max"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "GC pause rate (s/s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum{service=\"$service\"}[5m])",
          "refId": "A",
          "instant": false
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 6,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "JVM threads live",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "jvm_threads_live_threads{service=\"$service\"}",
          "refId": "A",
          "instant": false
        }
      ],
      "gridPos": {
        "x": 18,
        "y": 16,
        "w": 6,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      },
      "id": 10,
      "title": "Success rate (%)",
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"$service\",status=~\"2..\"}[5m])) / sum(rate(http_server_requests_seconds_count{service=\"$service\"}[5m])) * 100",
          "refId": "A",
          "instant": false,
          "legendFormat": "success"
        }
      ]
    },
    {
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      },
      "id": 11,
      "title": "Error rate (5xx) (%)",
      "gridPos": {
        "x": 12,
        "y": 24,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"$service\",status=~\"5..\"}[5m])) / sum(rate(http_server_requests_seconds_count{service=\"$service\"}[5m])) * 100",
          "refId": "A",
          "instant": false,
          "legendFormat": "5xx"
        }
      ]
    }
  ]
}
