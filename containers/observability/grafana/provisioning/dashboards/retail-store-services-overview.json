{
  "uid": "retail-store-services-overview",
  "title": "Retail Store - Services Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "retail-store"
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "label": "Service",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "query": "label_values(http_server_requests_seconds_count, service)",
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests per second",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (service) (rate(http_server_requests_seconds_count{service=~\"$service\"}[5m]))",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error rate (5xx) %",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (service) (rate(http_server_requests_seconds_count{service=~\"$service\",status=~\"5..\"}[5m])) / sum by (service) (rate(http_server_requests_seconds_count{service=~\"$service\"}[5m])) * 100",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, service) (rate(http_server_requests_seconds_bucket{service=~\"$service\"}[5m])))",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "CPU usage (%)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "process_cpu_usage{service=~\"$service\"} * 100",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Heap used (bytes)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{service=~\"$service\",area=\"heap\"}",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 8,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "JVM threads live",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "jvm_threads_live_threads{service=~\"$service\"}",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 8,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Hikari active connections",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "hikaricp_connections_active{service=~\"$service\"}",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "GC pause rate (s/s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum{service=~\"$service\"}[5m])",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 16,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "HTTP 2xx/4xx/5xx",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (status) (rate(http_server_requests_seconds_count{service=~\"$service\",status=~\"2..\"}[5m]))",
          "refId": "A",
          "instant": false,
          "legendFormat": "2xx"
        },
        {
          "expr": "sum by (status) (rate(http_server_requests_seconds_count{service=~\"$service\",status=~\"4..\"}[5m]))",
          "refId": "B",
          "instant": false,
          "legendFormat": "4xx"
        },
        {
          "expr": "sum by (status) (rate(http_server_requests_seconds_count{service=~\"$service\",status=~\"5..\"}[5m]))",
          "refId": "C",
          "instant": false,
          "legendFormat": "5xx"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 16,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "normal"
        }
      }
    },
    {
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      },
      "id": 10,
      "title": "Service up",
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "max by (job) (up{job=~\"ms-.*\"})",
          "refId": "A",
          "instant": false,
          "legendFormat": "{{job}}"
        }
      ]
    }
  ]
}
