{
  "uid": "retail-store-services",
  "title": "Retail Store - Services",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "retail-store"
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "label": "Service",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "query": "label_values(up{job=~\"ms-.*\"}, job)",
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Service up",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "up{job=~\"$service\"}",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 4
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Requests per second",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (job) (rate(http_server_requests_seconds_count{job=~\"$service\"}[5m]))",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Error rate (5xx) %",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (job) (rate(http_server_requests_seconds_count{job=~\"$service\",status=~\"5..\"}[5m])) / sum by (job) (rate(http_server_requests_seconds_count{job=~\"$service\"}[5m])) * 100",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 4,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "P95 latency (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, job) (rate(http_server_requests_seconds_bucket{job=~\"$service\"}[5m])))",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 4,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "CPU usage",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "process_cpu_usage{job=~\"$service\"}",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Heap used",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (job) (jvm_memory_used_bytes{job=~\"$service\", area=\"heap\"})",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 12,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Live threads",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "jvm_threads_live{job=~\"$service\"}",
          "refId": "A",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 12,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        },
        "stacking": {
          "mode": "none"
        }
      }
    }
  ]
}
